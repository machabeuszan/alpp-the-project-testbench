<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="testbench" />
		<Option pch_mode="2" />
		<Option compiler="avrgcc" />
		<Build>
			<Target title="Release">
				<Option output="bin/Release/testbench.elf" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="avrgcc" />
			</Target>
			<Target title="Debug">
				<Option output="bin/Debug/testbench.elf" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="avrgcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="MCU" value="atmega8" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-mmcu=atmega8" />
			<Add option="-Os" />
			<Add option="-Wall" />
			<Add option="-DF_CPU=16000000UL" />
			<Add directory="../release/include" />
		</Compiler>
		<Linker>
			<Add option="-Wl,-u,vfprintf" />
			<Add option="-lprintf_flt" />
			<Add option="-lm" />
			<Add library="../release/libalpp.a" />
		</Linker>
		<ExtraCommands>
			<Add after="avr-size $(TARGET_OUTPUT_FILE) -C --mcu=$(MCU)" />
			<Add after="avr-objcopy -O ihex -R .eeprom -R .eesafe $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).hex" />
			<Add after="avr-objcopy --no-change-warnings -j .eeprom --change-section-lma .eeprom=0 -O ihex $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).eep.hex" />
			<Add after="avr-objcopy -O binary -R .eeprom -R .eesafe $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).bin" />
			<Add after="avr-objcopy --no-change-warnings -j .eeprom --change-section-lma .eeprom=0 -O binary $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).eep.bin" />
			<Add after="avr-objdump -h -S $(TARGET_OUTPUT_FILE) &gt; $(TARGET_OUTPUT_FILE).lss" />
		</ExtraCommands>
		<Unit filename="main.cpp">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger>
				<remote_debugging target="Debug">
					<options conn_type="0" serial_baud="115200" ip_address="localhost" ip_port="1212" />
				</remote_debugging>
			</debugger>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
